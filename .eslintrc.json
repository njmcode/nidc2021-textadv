{
  "parser" : "@babel/eslint-parser",
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": "off",
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
    "no-underscore-dangle": "off"
  }
}
